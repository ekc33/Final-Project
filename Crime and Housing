    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "df = pd.read_csv(\"Arrests.csv\")\n",
    "\n",
    "r = df.groupby('INCIDENTNEIGHBORHOOD').filter(lambda x: len(x) < 50)\n",
    "\n",
    "incid = r['INCIDENTNEIGHBORHOOD'].value_counts()\n",
    "\n",
    "incid.plot.bar().set_title('Neighborhoods with less than 50 incidents')\n"
   ]
